<Application x:Class="RobotsWindows.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:RobotsWindows"
             StartupUri="CinematicWindow.xaml"
             
              xmlns:properties="clr-namespace:RobotsWindows.Properties"
              Exit="OnExit"
             >
    <Application.Resources>
        <ResourceDictionary>
            <properties:Settings x:Key="Settings" />
            
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Strings/lang.xaml" />
            </ResourceDictionary.MergedDictionaries>
            
        </ResourceDictionary>
    </Application.Resources>
</Application>
